@ECHO OFF
:GO_LOOP
ECHO A Run JLink
ECHO B Run ACTION2
ECHO C Run ACTION3
ECHO Q To Quit 
ECHO.
CHOICE /C:ABCQ /M "ECHO CHOICE[ABCQ]:"
	IF ERRORLEVEL 4 GOTO ACT_QUIT
	IF ERRORLEVEL 3 GOTO ACTION3
	IF ERRORLEVEL 2 GOTO ACTION2
	IF ERRORLEVEL 1 GOTO JLink
	GOTO ACT_QUIT
:JLink
	ECHO JLink choosed!
	JLink.exe -device A8107M0 -CommandFile CommandFile.jlink
	echo ret: %errorlevel%
	echo.
	GOTO GO_LOOP
:ACTION2
	ECHO ACTION2 choosed!
	ECHO.
	GOTO GO_LOOP
:ACTION3
	ECHO ACTION3 choosed!
	ECHO.
	GOTO GO_LOOP
:ACT_QUIT
	ECHO Quit choosed!

exit